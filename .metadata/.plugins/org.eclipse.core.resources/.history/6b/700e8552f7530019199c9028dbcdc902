package ejercicio3;

public class Elemento_Geometrico {
	private double coorX=0;
	private double coorY=0;
	
	public Elemento_Geometrico() {
	}
	
	public Elemento_Geometrico(double x,double y) {
		this.coorX=x;
		this.coorY=y;
	}
	
	public void desplazarPunto(double x,double y) {
		this.coorX=this.coorX+x;
		this.coorY=this.coorY+y;
	}

	public static void main(String[] args) {
		Elemento_Geometrico p1=new Elemento_Geometrico();
		p1.desplazarPunto(3, 4);
		System.out.println(p1);

	}

}
