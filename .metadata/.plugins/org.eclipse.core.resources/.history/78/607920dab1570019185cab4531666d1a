package ejercicio4;

import java.util.ArrayList;
import java.util.List;


public class Temporada {
	private List<Episodio> episodios=new ArrayList<>();
	
	public int episodiosVistosTemporada() {
		int contador=0;
		for (Episodio e:this.episodios) {
			if (e.getFlag()==true)
				contador++;
		}
		return contador;
	
	}
}
	